            <kind>1</kind>
            <string>/// Dibujar hechizo
draw_self();
alpha += delta;

if (casteando) {
    var size = string_length(hechizo);
    draw_set_colour(c_red);
    draw_set_alpha(.1+alpha*.2);
    
    for (var i = 1; i &lt; size; i++) {
        var numero = real(string_char_at(hechizo, i));
        var xorig  = puntos[numero-1].x;
        var yorig  = puntos[numero-1].y;
        
        numero = real(string_char_at(hechizo, i+1));
        var xdest  = puntos[numero-1].x;
        var ydest  = puntos[numero-1].y;
        
        draw_line_width(xorig, yorig, xdest, ydest, 10);
        draw_line_width(xorig, yorig, xdest, ydest,  7);
        draw_line_width(xorig, yorig, xdest, ydest,  4);
        draw_line_width(xorig, yorig, xdest, ydest,  1);
    }
    
    if (size &gt; 0 &amp;&amp; size &lt; array_length_1d(puntos)) {
        var numero = real(string_char_at(hechizo, size));
        var xorig  = puntos[numero-1].x;
        var yorig  = puntos[numero-1].y;
        var xdest  = mouse_x;
        var ydest  = mouse_y;
        
        draw_line_width(xorig, yorig, xdest, ydest, 10);
        draw_line_width(xorig, yorig, xdest, ydest,  7);
        draw_line_width(xorig, yorig, xdest, ydest,  4);
        draw_line_width(xorig, yorig, xdest, ydest,  1);
    }
}
draw_set_alpha(1);
draw_set_colour(c_black);

