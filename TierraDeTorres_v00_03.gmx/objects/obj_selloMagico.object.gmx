            <string>/// Castear hechizo
if (casteando) {
    var completo = string_length(hechizo) == 6;
    
    if (completo) {
        if (obj_personaje.orbes &gt; 0)
            obj_personaje.orbes -= 1;
        var mapaHechizos = ds_map_create_read(global.formasHechizos);
